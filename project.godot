config_version=5

[application]

config/name="Proyecto Final Taller"
run/main_scene="res://GUI/main.tscn"
config/features=PackedStringArray("4.2", "C#", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Global="*res://Settings/Autoload/Global.cs"

[display]

window/size/viewport_width=384
window/size/viewport_height=216
window/size/mode=2
window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="Proyecto Final Taller"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
